            <div class="col-md-4">
              <article class="icontext align-items-start">
                <span class="icon icon-sm rounded-circle bg-primary-light">
                  <i class="text-primary material-icons md-place"></i>
                </span>
                <div class="text">
                  <h6 class="mb-1">Deliver to</h6>
                  <p class="mb-1">
                    <%= singleOrder[0].shippingAddress.name %>
                    <br><%= singleOrder[0].shippingAddress.firstLine %><br><%= singleOrder[0].shippingAddress.secondLine %>
                    <%= singleOrder[0].shippingAddress.city %><br><%= singleOrder[0].shippingAddress.state %><br>
                    <%= singleOrder[0].shippingAddress.pincode %>
                  
                  
                  
                    <!-- <a href="/profile">View profile</a> -->
                </div>
              </article>
            </div> 
          </div> 
          <div class="row">
            <div class="col-lg-7">
              <div class="table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                      <th width="40%">Product</th>
                      <th width="20%">Quantity</th>
                      <th width="20%">UnitPrice</th>
                      
                    </tr>
                  </thead>
                  <tbody>
                    <% for(let i=0; i<singleOrder[0].products.length; i++) { %>
                    <tr>
                      <td>
                        <a class="itemside" href="#">
                          <div class="left">
                            <img src="/images/<%= singleOrder[0].products[i].product.Image[0] %>" width="40" height="40" class="img-xs"  
                            alt="Item">
                            <span style="color: black; font-weight: bolder;"><%= singleOrder[0].products[i].product.Productname%></span>
  
                          </div>
                          <div class="info"><%= singleOrder.name %></div>
                        </a>
                      </td>

                      <td><%= singleOrder[0].products[0].quantity%></td>
                      <td>₹<%= singleOrder[0].totalPrice%></td>
                        
                      
  
                    </tr>
                    <% } %>
                    <tr>
                      <td colspan="4">
                        <article class="float-end">
                          <dl class="dlist">
                            <dt>Grand total:</dt>
                            <dd><b class="h5">₹<%= singleOrder[0].totalPrice %></b></dd>
                          </dl>
                        </article>
                      </td>
                    </tr>
                  </tbody>
                  
                </table>
              </div> <!-- table-responsive// -->
            </div> <!-- col// -->
            <div class="col-lg-1"></div>
            <div class="col-lg-4">
              <div class="box shadow-sm bg-light">
                <h6 class="mb-15">Payment info</h6>
                <p>
                  <%= singleOrder[0].paymentMethod%>
                </p>
                <br>
                <br>
                
              </div>
              <% if(singleOrder[0].status === "cancelled") { %>
                <h4>Order cancelled</h4>
              <% } else { %>
                <a href="/order-cancel/<%= singleOrder[0]._id %>" class="btn btn-danger" onclick="return confirm('Do you want to cancel this order? Are you sure?')">Cancel Items</a>
              <% } %>
                <br>
                <br>
                <!-- <button style="background-color: black;color: white";>Return Order</button> -->
              <div class="h-25 pt-4">
                
              </div>

             
              

  
              <!-- col// -->
            </div>
