<section>
    <div class="container">
      <table class="table mt-5" id="ordersTable">
        <thead>
          <tr>
            <th scope="col">OrderId</th>
            <th scope="col">Date</th>
            <th scope="col">Payment</th>
            <th scope="col">Status</th>
            <th scope="col">Total</th>
            <th scope="col">Actions</th>
          </tr>
        </thead>
        <tbody> 
          <% orders.forEach(function(response) { %>
            <tr>
                <td><%=response.uniqueId%></td>

              <td><%=getDate(response.date) %></td>
              <td><%= response.paymentMethod %></td>
              <td><%= response.status %></td>
              <td>â‚¹<%= response.totalPrice %></td>
    
              <td>
                <a href="/userprofileordersview1/<%=response._id%>" class="btn btn-primary">View</a>
              </td>
            </tr>
<%})%>
          </tbody>
      </table>
    </div>
</section>
